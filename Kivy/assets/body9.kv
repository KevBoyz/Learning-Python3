#: import XCamera kivy.garden.xcamera.XCamera

FloatLayout:
    orientation:  'vertical'
    XCamera:
        id: xcamera
        on_picture_taken: app.pic_taken()
    Button:
        text: 'Change camera'
        size_hint: .2, .1
        pos_hint: {'center_x': .5}
        on_release: app.change_cam(*args)