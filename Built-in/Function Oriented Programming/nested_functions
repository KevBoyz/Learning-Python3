from difflib import ndiff
from typing import List, NoReturn

ta = """\
hello
my 
name 
is 
Xanes
"""
tb = """\
hallo
my 
name 
is 
Chains
"""


def diff_txt() -> NoReturn:

    def read_file(file: str) -> List:
        return open(file).readlines()

    #print(ndiff(read_file(ta), read_file(tb)))
    print(''.join(ndiff(ta, tb)))


diff_txt()
